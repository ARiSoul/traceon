<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Arisoul.Traceon.App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Arisoul.Traceon.App"
    xmlns:views="clr-namespace:Arisoul.Traceon.App.Views"
    Title="Traceon.App"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Actions"
        ContentTemplate="{DataTemplate views:TrackedActionsPage}"
        Route="TrackedActionsPage" />

</Shell>
